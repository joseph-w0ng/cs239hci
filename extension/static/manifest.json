{
	"name": "Cookie Monster",
	"description": "Displays the current active cookies for the active website.",
	"version": "1.1.0",
	"manifest_version": 3,
	"action": {
		"default_title": "View Active Cookies",
		"default_icon": "favicon.png",
		"default_popup": "index.html"
	},
	"background": {
		"service_worker": "service-worker.js",
		"type": "module"
	},
	"permissions": ["cookies", "storage", "scripting", "activeTab", "storage"],
	"host_permissions": ["<all_urls>"]
}
